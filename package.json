{
  "name": "su95x",
  "version": "0.4.0",
  "edition": "development",
  "scripts": {
    "====== SU95X =================": "==========================================",

    "build-su95x:copy-base-package": "mkdir -p build-su95x/out/headwindsim-aircraft-su100-95 && (rsync -a build-su95x/src/base/headwindsim-aircraft-su100-95 build-su95x/out/ || cp -a -u build-su95x/src/base/headwindsim-aircraft-su100-95 build-su95x/out/)",
    "build-su95x:copy-base-lock-highlight-package": "mkdir -p build-su95x/out/headwindsim-aircraft-su100-95-lock-highlight && (rsync -a build-su95x/src/base/headwindsim-aircraft-su100-95-lock-highlight build-su95x/out/ || cp -a -u build-su95x/src/base/headwindsim-aircraft-su100-95-lock-highlight build-su95x/out/)",
    "build-su95x:copy-base-files": "npm run build-su95x:copy-base-package && npm run build-su95x:copy-base-lock-highlight-package",
    "build-su95x:efb-translation": "cd build-su95x/src/systems/instruments/src/EFB/Localization && node build-translation.js",

    "build-su95x:model": "node build-su95x/src/model/build.js",
    "build-su95x:behavior": "node build-su95x/src/behavior/build.js",

    "build-su95x:atsu": "rollup -c build-su95x/src/systems/atsu/rollup.config.js",
    "build-su95x:failures": "rollup -c build-su95x/src/systems/failures/rollup.config.js",
    "build-su95x:fmgc": "rollup -c build-su95x/src/systems/fmgc/rollup.config.js",
    "build-su95x:sentry-client": "rollup -c build-su95x/src/systems/sentry-client/rollup.config.js",
    "build-su95x:simbridge-client": "rollup -c build-su95x/src/systems/simbridge-client/rollup.config.js",
    "build-su95x:tcas": "rollup -c build-su95x/src/systems/tcas/rollup.config.js",
    "build-su95x:pfd": "cd build-su95x/src/systems/instruments/src/PFD && rollup -c",

    "build-su95x:systems": "cargo build -p a320_systems_wasm --target wasm32-wasi --release && wasm-opt -O1 -o /external/build-su95x/out/headwindsim-aircraft-su100-95/SimObjects/AirPlanes/FlyByWire_A320_NEO/panel/systems.wasm /external/target/wasm32-wasi/release/a320_systems_wasm.wasm",
    "build-su95x:fadec": "cd build-su95x/src/wasm/fadec_a320 && ./build.sh && wasm-opt -O1 -o /external/build-su95x/out/headwindsim-aircraft-su100-95/SimObjects/AirPlanes/FlyByWire_A320_NEO/panel/fadec.wasm /external/build-su95x/out/headwindsim-aircraft-su100-95/SimObjects/AirPlanes/FlyByWire_A320_NEO/panel/fadec.wasm",
    "build-su95x:fbw": "cd build-su95x/src/wasm/fbw_a320 && ./build.sh && wasm-opt -O1 -o /external/build-su95x/out/headwindsim-aircraft-su100-95/SimObjects/AirPlanes/FlyByWire_A320_NEO/panel/fbw.wasm /external/build-su95x/out/headwindsim-aircraft-su100-95/SimObjects/AirPlanes/FlyByWire_A320_NEO/panel/fbw.wasm",
    "build-su95x:flypad-backend": "cd build-su95x/src/wasm/flypad-backend && ./build.sh && wasm-opt -O1 -o /external/build-su95x/out/headwindsim-aircraft-su100-95/SimObjects/AirPlanes/FlyByWire_A320_NEO/panel/flypad-backend.wasm /external/build-su95x/out/headwindsim-aircraft-su100-95/SimObjects/AirPlanes/FlyByWire_A320_NEO/panel/flypad-backend.wasm",

    "build-su95x:metadata": "node scripts/metadata.js build-su95x/out/headwindsim-aircraft-su100-95 su95x",
    "build-su95x:manifest": "node scripts/build_su95x.js",


    "serve:efb": "cd build-su95x/src/systems/instruments/src/EFB/ && vite --port 9696",
    "build:ace": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c .\\build-su95x\\src\\systems\\instruments\\buildSrc\\aceBuild.mjs",
    "watch:ace": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -wc .\\build-su95x\\src\\systems\\instruments\\buildSrc\\aceBuild.mjs",
    "build:instruments": "rollup --max-old-space-size=8192 -c build-su95x/src/systems/instruments/buildSrc/simulatorBuild.mjs",
    "watch:instruments": "rollup --max-old-space-size=8192 -wc build-su95x/src/systems/instruments/buildSrc/simulatorBuild.mjs",

    "prettier": "prettier --write **/*.json **/*.yml build-su95x/src/systems/instruments/**/*.css",

    "build-su95x:all": "npm run build-su95x:copy-base-files && npm run build-su95x:efb-translation && npm run build-su95x:model && npm run build-su95x:behavior && npm run build-su95x:atsu && npm run build-su95x:failures && npm run build-su95x:fmgc && npm run build-su95x:sentry-client && npm run build-su95x:simbridge-client && npm run build-su95x:tcas && npm run build-su95x:pfd && npm run build-su95x:systems && npm run build-su95x:fadec && npm run build-su95x:fbw && npm run build-su95x:flypad-backend && npm run build-su95x:metadata && npm run build-su95x:manifest",

    "====== COMMON ================": "==========================================",

    "lint": "eslint --cache **/*.{js,mjs,jsx,ts,tsx}",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,mjs,jsx}": "eslint --cache --fix",
    "*.{json,yml,css,scss}": "prettier --write"
  },
